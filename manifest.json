{
  "manifest_version": 2,

  "name": "Emotion Tracker",
  "description": "Trackes facial expressions using the webcam",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "options_ui": {
    "page": "popup.html",
    "chrome_style": true,
    "open_in_tab": true
  },

  "permissions": [
    "tabs"
  ]
}
